{
  "groupDescriptions": {
    "the_vault:horde": {
      "description": [
        {
          "text": "Horde ",
          "color": "$name"
        },
        {
          "text": "consists of mobs that will spawn commonly in large quantities, they have lower stats overall.",
          "color": "$text"
        }
      ]
    },
    "the_vault:assassin": {
      "description": [
        {
          "text": "Assassin ",
          "color": "$name"
        },
        {
          "text": "mobs are high damaging less common mobs that are a threat to be taken seriously. They will have higher damage stats than Horde mobs.",
          "color": "$text"
        }
      ]
    },
    "the_vault:tank": {
      "description": [
        {
          "text": "Tanks ",
          "color": "$name"
        },
        {
          "text": "are high HP sponges that will test your single target strength, they will however spawn in much smaller quantities so you can focus them.",
          "color": "$text"
        }
      ]
    },
    "the_vault:dungeon": {
      "description": [
        {
          "text": "Dungeon ",
          "color": "$name"
        },
        {
          "text": "mobs are much stronger than their regular variants and scale with the dungeons difficulty.",
          "color": "$text"
        }
      ]
    },
    "the_vault:fighter": {
      "description": [
        {
          "text": "Vault Dwellers ",
          "color": "$name"
        },
        {
          "text": "are the denizens of the vault and will provide environmental threats to the player in difficulties greater than Hard.",
          "color": "$text"
        }
      ]
    }
  },
